{
    "name": "Phluffstravels",
    "version": "0.1.0",
    "description": "Geolocation app to visualize your attended shows.",
    "main": "lib/index.js",
    "module": "es/index.js",
    "jsnext:main": "es/index.js",
    "scripts": {
        "clean": "rimraf lib es dist",
        "build:cjs": "babel src --out-dir lib",
        "build:es": "cross-env BABEL_ENV=jsnext babel src --out-dir es",
        "build": "npm run clean && npm run build:cjs && npm run build:es",
        "lint": "eslint ./src",
        "start": "npm run build",
        "server": "node src/server.js",
        "dev": "node src/server --config ./src/webpack.config.babel.js --progress"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/b-pmcg/ptravels"
    },
    "keywords": [
        "react-component",
        "react",
        "leaflet",
        "map",
        "geolocation"
    ],
    "author": "Phil Bain <bain.pmcg@gmail.com>",
    "license": "MIT",
    "homepage": "https://www.phluffstravels.com.com",
    "dependencies": {
        "axios": "^0.16.2",
        "babel-plugin-transform-regenerator": "^6.26.0",
        "babel-polyfill": "^6.26.0",
        "express": "^4.16.0",
        "html-react-parser": "^0.3.5",
        "html-webpack-plugin": "^2.30.1",
        "lodash": "^4.0.0",
        "lodash-es": "^4.0.0",
        "react-leaflet": "^1.7.0",
        "react-leaflet-control": "^1.4.0",
        "react-player": "^0.24.5",
        "request": "^2.83.0",
        "request-promise": "^4.2.2",
        "router": "^1.3.2",
        "warning": "^3.0.0",
        "webpack-dev-middleware": "^1.12.0",
        "webpack-hot-middleware": "^2.19.1"
    },
    "peerDependencies": {
        "leaflet": "^1.2.0",
        "prop-types": "^15.5.0",
        "react": "^15.0.0",
        "react-dom": "^15.0.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^7.2.3",
        "babel-jest": "^21.2.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-dev-expression": "^0.2.1",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-plugin-lodash": "^3.2.11",
        "babel-plugin-transform-proto-to-assign": "^6.26.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-1": "^6.24.1",
        "cross-env": "^5.0.5",
        "eslint": "^4.7.2",
        "eslint-config-prettier": "^2.6.0",
        "eslint-plugin-flowtype": "^2.36.0",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-node": "^5.2.0",
        "eslint-plugin-prettier": "^2.3.1",
        "eslint-plugin-promise": "^3.5.0",
        "eslint-plugin-react": "^7.4.0",
        "leaflet": "^1.2.0",
        "prettier": "^1.7.2",
        "prop-types": "^15.6.0",
        "react": "^15.6.2",
        "react-dom": "^15.6.2",
        "rimraf": "^2.6.2",
        "webpack": "^3.6.0",
        "webpack-dev-server": "^2.9.1"
    }
